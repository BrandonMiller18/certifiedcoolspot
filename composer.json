{
    "name": "bh/wp-content",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "wpackagist-plugin/wp-term-images":"1.0.0",
        "wpackagist-plugin/font-awesome":"4.4.0",
        "wpackagist-plugin/classic-editor":"1.6.3",
        "wpackagist-plugin/wordpress-seo":"20.12"
    },
    "extra": {
        "installer-paths": {
            "plugins/{$name}": [
                "type:wordpress-plugin"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
